{% extends 'base.html.twig' %}

{% block title %}Liste utilisteur{% endblock %}

{% block body %}
     <h1>Liste utilisteurs</h1>
	 <table class="table">
	    <thead>
		<th>id</th>
		<th>Email</th>
		<th>Nom</th>
		<th>Prenom</th>
		<th>supprimer</th>
	</table>
	<tbody> 
	{for user in users %}
	<tr>
	    <td>{{ user.id}}</td>
		<td>{{user.Email}}</td>
		<td>{{user.Nom}}</td>
		<td>{{user.Prenom}}</td>
		<td>{{user.supprimer}}</td>
	 </tr>
	{ % else %}
	<tr>
	 <td colspan=5>il n'y a pas d'utilisateur inscrit </td>
	 </tr>
	</tbody>
	 
{% endblock %}
